<root>
	<styles>
		<include src="file://{resources}/styles/steamstyles.css" />
		<include src="file://{resources}/styles/music/music.css" />
		<include src="file://{resources}/styles/music/music_library.css" />
	</styles>
	
	<MusicLibraryAlbumPanel onmoveup="MusicLibraryActivateGridTabs( AlbumButton );"
			defaultfocus="MusicAlbumGrid" selectionposboundary="vertical" >
		
		
		<Panel hittest="false" id="FastScrollWrapper">
			<Label id="FastScrollLabel" />
			<Label id="TypeToJumpLabel" />
		</Panel>
		
		<Grid id="MusicAlbumGrid" class="MusicAlbumGrid" 
			scrolldirection="horizontal" verticalcount="2" horizontalcount="5" scaleoffset="0.78528" selectionpos="0,3" tabindex="auto">
		</Grid>
		
	</MusicLibraryAlbumPanel>
</root>
